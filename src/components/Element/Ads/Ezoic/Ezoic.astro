const { content, template, data, id } = Astro.props;
---

{(template == "banner") && <EzoicBanner data={data} />}
{(template == "box") && <EzoicBox data={data} />}
{(template == "footer") && <EzoicFooter data={data} />}
{(template == "card") && <EzoicCard data={data} />}
{(template == "flexbanner") && <EzoicFlexBanner data={data} />}
{(template == "undertitle") && <EzoicUnderTitle data={data} />}
