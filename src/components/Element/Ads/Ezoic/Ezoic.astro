---
//*             [EZOIC]
//?             Advertisement Platform.
//?             ^POST: Adding the recommended amount of Ezoic Elements.
import EzoicBanner from "./EzoicBanner.astro"; //?              {Ezoic}:[112]
import EzoicFooter from "./EzoicFooter.astro"; //?              {Ezoic}:[113]
import EzoicBox from "./EzoicBox.astro"; //?                    {Ezoic}:[114]
import EzoicCard from "./EzoicCard.astro"; //?                  {Ezoic}:[115]
import EzoicFlexBanner from "./EzoicFlexBanner.astro"; //?      {Ezoic}:[116]
import EzoicUnderTitle from "./EzoicUnderTitle.astro"; //?      {Ezoic}:[117]
import EzoicInContent from "./EzoicInContent.astro"; //?        {Ezoic}:[118]
import EzoicContent from "./EzoicContent.astro"; //?            {Ezoic}:[119]
import EzoicSmallBox from "./EzoicSmallBox.astro"; //?          {Ezoic}:[120]
import EzoicMidBanner from "./EzoicMidBanner.astro"; //?        {Ezoic}:[121]
import EzoicMDX from "./EzoicMDX.astro"; //?         {Ezoic}:[123]
//?             [DATA]
const { content, template, data, id, className = "" } = Astro.props;
---

{template == "mdx" && <EzoicMDX data={data} />}
{template == "banner" && <EzoicBanner data={data} />}
{template == "box" && <EzoicBox data={data} />}
{template == "footer" && <EzoicFooter data={data} />}
{template == "card" && <EzoicCard data={data} />}
{template == "flexbanner" && <EzoicFlexBanner data={data} />}
{template == "undertitle" && <EzoicUnderTitle data={data} />}
{template == "incontent" && <EzoicInContent data={data} />}
{template == "content" && <EzoicContent data={data} className={className} />}
{
  template == "smallbox" && className != "" && (
    <EzoicSmallBox data={data} className={className} />
  )
}
{
  template == "midbanner" && className != "" && (
    <EzoicMidBanner data={data} className={className} />
  )
}
