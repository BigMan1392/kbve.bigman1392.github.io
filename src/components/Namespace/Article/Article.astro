---
//?     [NS]    ->      [Article]
const {
    id,
    element = "astro",
    articleTemplate,
    articleClass,
    articleData,
    articleJSON,
    articleMD
} = Astro.props;

import MambaArticle from "@c/Element/React/ReactArticle/MambaArticle/MambaArticle.astro";
import AstroArticle from "@c/Element/Article/Article.astro";
---

{(element == "astro") && <AstroArticle id={id} articleTemplate={articleTemplate} articleJSON={articleJSON} articleClass={articleClass} articleMD={articleMD} />}
{(element == "react") && <MambaArticle id={id} articleTemplate={articleTemplate} articleJSON={articleJSON} articleClass={articleClass} articleMD={articleMD} />}