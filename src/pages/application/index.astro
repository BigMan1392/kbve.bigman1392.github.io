---
import N from "@n/N.astro";             //      [NameSpace]
let title = "KBVE Applications ToolBelt";
let description = "Collection of Applications that KBVE utilizes within our toolbelt";

---
<N ns="theme" title={title} description={description}>
  <N ns="banner" template="sale" />
  <!-- Widget Test -->
  <script is:inline crossorigin="anonymous" type="text/partytown" src="https://kbve.github.io/widget-react-application/index.js"></script>
  <link rel="stylesheet" href="https://kbve.github.io/widget-react-application/index.css" />
  <div class="widget_react_application" data-limit="50" data-json="application"></div>
  <!-- Widget Untest -->
  <N ns="controller" template="application" />
  <N ns="ads" template="midbanner" id="ezoic" />
</N>