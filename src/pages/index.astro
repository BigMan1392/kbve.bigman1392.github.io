import SHash from "@c/Tools/SHash";
//<!-- *       [MIGRATION]: -> [@Components]
import Splash from "@c/Splash/splash.astro"; //<!-- ! [SPLASH]    ->  Splash is still giving issues as of 1/2/2022.
