---
///				[404]:Template - The default 404 template to be rendered for 404 errors.
///				[PLAN]: Integrate the {N} : Namespace.
//?				[PLAN]: Display search for missing files, thus helping the user navigate to the right page.
import Core from "@l/theme/core.astro";					///	[IMPORT]	->	[CORE]
import SWUP from "@l/theme/swup.astro";					///	[SWUP]		->	Disabled;

const data = {											///	[Data]		->	();
	title: "Not Found", description: "Not Found"}		///	[404]		->	Data
const { content = data } = Astro.props;					///	[Astro]		->	[Props]
Astro.response.status = 404;							/// [Astro]		->	[Response]	-> 404
Astro.response.statusText = 'Not found';				///	[Astro]		->	[Response]	-> 'Not found'

import Robo404 from "@c/Element/Lottie/404/404.astro";	///	[Lottie]	->	404 Animation
---
<Core content={content}>
	<SWUP>
		<div class="px-4 py-12 space-y-8">
			<br />
			<h1>Page Not Found</h1>
			<p>Not found</p>
			<Robo404 />
		</div>
	</SWUP>
</Core>
